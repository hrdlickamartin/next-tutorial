{
  "name": "test",
  "scripts": {
    "start": "nodemon -e js,graphql -x node -r dotenv/config index.js",
    "debug":
      "nodemon -e js,graphql -x node --inspect -r dotenv/config index.js",
    "playground": "graphql playground --port 5000",
    "dev": "npm-run-all --parallel start playground",
    "deps":
      "rimraf node_modules yarn.lock flow-typed/npm && yarn && flow-typed install"
  },
  "dependencies": {
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "bcryptjs": "2.4.3",
    "graphql-yoga": "1.2.1",
    "jsonwebtoken": "8.1.1",
    "prisma-binding": "1.5.0"
  },
  "devDependencies": {
    "dotenv": "5.0.0",
    "graphql-cli": "2.13.0",
    "nodemon": "1.14.12",
    "npm-run-all": "4.1.2",
    "prisma": "1.1.2"
  }
}
